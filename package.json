{
	"name": "arm",
	"displayName": "Arm Assembly (UPDATED)",
	"description": "Arm assembly syntax support for Visual Studio Code",
	"version": "1.7.5",
	"publisher": "ciprianelies",
	"icon": "images/icon.png",
	"preview": true,
	"author": {
		"name": "Dan Underwood"
	},
	"categories": [
		"Programming Languages"
	],
	"engines": {
		"vscode": "^0.10.1"
	},
	"galleryBanner": {
		"color": "#0084A9",
		"theme": "dark"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ciprianelies/vscode-arm.git"
	},
	"contributes": {
		"languages": [
			{
				"id": "arm",
				"aliases": [
					"ARM",
					"arm",
					"Arm"
				],
				"extensions": [
					".s",
					".S",
					".asm",
					".sx"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "arm",
				"scopeName": "source.arm",
				"path": "./syntaxes/arm.tmlanguage.json"
			}
		]
	}
}
